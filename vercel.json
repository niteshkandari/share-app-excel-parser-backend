{
  "version": 2,
  "builds": [
    { "src": "src/server.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/prisma/schema.prisma" , "uploads"]}
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.ts"
    }
  ],
  "buildCommand": "npm run build"
}
